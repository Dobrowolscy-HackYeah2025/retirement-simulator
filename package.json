{
  "name": "retirement-simulator",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:with-check": "tsc -b && vite build",
    "build:spa": "npm run build && echo 'SPA build complete - ready for static hosting'",
